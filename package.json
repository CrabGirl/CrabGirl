{
  "name": "crabgirl.hoffert.org",
  "version": "0.0.1",
  "description": "Crab Girl",
  "author": "Mike Hoffert",
  "scripts": {
    "build": "webpack --env.production && cp _redirects public/",
    "lint": "eslint src",
    "start": "./node_modules/.bin/webpack-dev-server --env",
    "test": "NODE_ENV=test TZ=UTC jest --modulePaths=src"
  },
  "dependencies": {
    "@emotion/babel-preset-css-prop": "10.0.27",
    "@emotion/core": "10.0.28",
    "@emotion/styled": "10.0.27",
    "@fortawesome/fontawesome": "^1.1.5",
    "@fortawesome/fontawesome-free-brands": "^5.0.10",
    "@fortawesome/fontawesome-free-solid": "^5.0.10",
    "@fortawesome/react-fontawesome": "0.0.18",
    "classnames": "2.2.6",
    "jsdom": "15.2.1",
    "lodash": "4.17.19",
    "moment": "2.29.2",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-apollo": "3.1.3",
    "react-dom": "16.12.0",
    "react-icons": "3.8.0",
    "react-onclickoutside": "6.9.0",
    "react-redux": "7.1.3",
    "react-router": "5.1.2",
    "react-router-dom": "5.1.2",
    "react-table": "7.0.0-rc.16",
    "redux": "4.0.4"
  },
  "devDependencies": {
    "@babel/cli": "7.7.0",
    "@babel/core": "7.7.2",
    "@babel/node": "7.7.0",
    "@babel/plugin-proposal-class-properties": "7.7.0",
    "@babel/plugin-proposal-object-rest-spread": "7.6.2",
    "@babel/polyfill": "7.7.0",
    "@babel/preset-env": "7.7.1",
    "@babel/preset-react": "7.7.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "10.0.3",
    "babel-jest": "24.9.0",
    "babel-loader": "8.0.6",
    "babel-plugin-emotion": "10.0.33",
    "babel-plugin-graphql-tag": "2.5.0",
    "babel-plugin-transform-runtime": "6.23.0",
    "clean-webpack-plugin": "3.0.0",
    "enzyme": "3.10.0",
    "enzyme-adapter-react-16": "1.15.1",
    "enzyme-to-json": "3.4.3",
    "eslint": "6.6.0",
    "eslint-config-standard": "14.1.0",
    "eslint-import-resolver-webpack": "0.11.1",
    "eslint-loader": "3.0.2",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jest": "23.0.3",
    "eslint-plugin-jsdoc": "20.0.0",
    "eslint-plugin-node": "10.0.0", "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-react": "7.16.0",
    "eslint-plugin-react-hooks": "2.3.0",
    "eslint-plugin-standard": "4.0.1",
    "file-loader": "4.2.0",
    "html-webpack-plugin": "3.2.0",
    "jest": "24.9.0",
    "jest-emotion": "10.0.17",
    "jest-runtime": "24.9.0",
    "jest-styled-components": "6.3.4",
    "react-test-renderer": "16.12.0",
    "regenerator-runtime": "0.11.1",
    "webpack": "4.41.2",
    "webpack-bundle-analyzer": "3.6.0",
    "webpack-cli": "3.3.10",
    "webpack-dev-server": "3.9.0",
    "webpack-serve": "3.2.0"
  },
  "jest": {
    "clearMocks": true,
    "setupFiles": [
      "<rootDir>/testSetup.js"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/testFrameworkScriptFile.js"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.js",
      "!src/index.js",
      "!src/App.js",
      "!src/Router.js",
      "!src/reduxStore/index.js"
    ],
    "testMatch": [
      "**/*.test.js"
    ],
    "testURL": "http://localhost",
    "snapshotSerializers": [
      "enzyme-to-json/serializer",
      "jest-emotion"
    ]
  }
}
